class Solution(object):
    def findLonelyPixel(self, picture):
        """
        :type picture: List[List[str]]
        :rtype: int
        """
        #This method is not the fastest, I'm just curious if it would work.
        if not picture:
            return 0
        length=len(picture)
        counter=0
        rowcount=0
        indices=[]
        i=0
        while i<length:
            print i
            j=0
            while j<length:
                print picture[i]
                if picture[i][j]=='B':
                    print "We gotta B real"
                    rowcount+=1
                    indices.append([j,i])
                    print indices
                    if rowcount>1:
                        indices=indices[:-2]
                        j=length
                        #print indices
                #rowcount=0        
                j+=1
            rowcount=0
            i+=1
        validrow=[]    
        #now we have the B's that are  alone in their row
        #need to narrow it down to the ones alone in their column
        for k in indices:
            print indices
            print k[0]
            if k[0] not in validrow:
                validrow.append(k[0])
            if k[0] in validrow:
                validrow.remove(k[0])
        print len(indices)
